@{
  Layout = "";
}
@model IEnumerable<string>

@if (Model.Count() != 0)
{
    <div class="widget">
        <h2 class="widget-title">@Resources.PersianResources.Keywords</h2>
        <div class="tagcloud">
            @foreach (var item in Model)
            {
                <a href="/Search/@item" title="@item">@item</a>
            }
        </div>
    </div>
}
